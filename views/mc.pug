doctype html
head
  title Multiple Choice
  link(rel="stylesheet", href="../style.css")
body
  h1 Multiple Choice:
  hr
  div.maincontain
    form(method="post")
      p
        br
        label.qcontain #{question}
        div.acontain
          for i in options
            input(type="radio", name="a",id="a"+i value=i)
            -console.log(i);
            label(for="a"+i) #{i}
            br
        input(type="submit")
